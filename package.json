{
    "name": "anix",
    "version": "0.1.8",
    "license": "BSD",
    "keywords": [
        "react",
        "motion",
        "animation",
        "animate"
    ],
    "main": "src/index.ts",
    "homepage": "https://github.com/a-jie/AniX",
    "scripts": {
        "tslint": "tslint -c tslint.json 'src/*.ts'",
        "compile_anix": "tsc ./src/index.ts -m commonjs --outDir ./dist/anix --lib es5,es2015.iterable,es2015.collection,es2015.promise,dom --sourceMap --target ES5 -d --diagnostics --pretty --noImplicitAny --noImplicitReturns --noImplicitThis --suppressImplicitAnyIndexErrors --moduleResolution node",
        "compile_react": "tsc ./src/index.ts -m es2015 --sourceMap --outDir ./dist/react --target ES5 -d --diagnostics --pretty --noImplicitAny --noImplicitReturns --noImplicitThis --suppressImplicitAnyIndexErrors --moduleResolution node --noEmitHelpers --lib es5,es2015.iterable,es2015.collection,es2015.promise,dom ",
        "compile_jq": "tsc ./src/index.ts -m amd --sourceMap --out ./dist/jq/anix.js --target ES5 --diagnostics --pretty --noImplicitAny --noImplicitReturns --noImplicitThis --suppressImplicitAnyIndexErrors --moduleResolution node --noEmitHelpers --lib es5,es2015.iterable,es2015.collection,es2015.promise,dom ",
        "clean_anix": "shx rm -rf ./dist/anix",
        "clean_react": "shx rm -rf ./dist/react",
        "clean_jq": "shx rm -rf ./dist/jq",
        "anix": "npm-run-all clean_anix compile_anix",
        "react": "npm-run-all clean_react compile_react",
        "jq": "npm-run-all clean_jq compile_jq",
        "package": "node .makepackage.js",
        "pub_anix": "cd ./dist/anix && npm publish",
        "all": "npm-run-all react anix jq package"
    },
    "dependencies": {},
    "devDependencies": {
        "npm-run-all": "^4.0.2",
        "shx": "^0.2.2",
        "tslint": "^5.2.0",
        "typescript": "^2.3.2"
    }
}